{
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "no-unused-vars": "warn",
      "prefer-const": "warn",
      "quotes": ["warn", "single"],
      "jsx-quotes": ["warn", "prefer-double"],
      "indent": ["warn", 2],
      "max-len": ["warn", {"code": 120}],
      "comma-dangle": ["warn", "always-multiline"],
      "semi": ["warn", "always"],
      "import/order": ["warn", {
        "groups": ["external", "internal", "parent", "sibling", "index", "object", "type"],
        "newlines-between": "always-and-inside-groups"
      }]
    }
}